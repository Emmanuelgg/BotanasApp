<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".SellFragment" android:id="@+id/frameLayout"
                                                   tools:layout_editor_absoluteY="81dp">
    <Button
            android:text="@string/show_all"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/showAll"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.35000002"
            app:layout_constraintHorizontal_bias="0.99" android:background="@drawable/btn_accent_small"
            android:textColor="@color/colorWhite" android:paddingLeft="2dp"
            android:paddingTop="4dp" android:paddingRight="2dp" android:paddingBottom="4dp"
            android:layout_marginTop="8dp" android:layout_marginBottom="8dp" android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"/>
    <TableLayout
            android:layout_width="match_parent"
            android:layout_height="275dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/tableLayout4">

        <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            <TextView
                    android:text="@string/sf_quantity"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView8"
                    android:paddingLeft="4dp"
                    android:paddingTop="8dp" android:paddingRight="4dp"
                    android:paddingBottom="8dp" android:background="@color/colorTableHeader" android:textColor="#FFFFFF"
                    android:fontFamily="@font/viga" android:layout_weight=".2" android:gravity="center"/>
            <TextView
                    android:text="@string/sf_product_name"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView6"
                    android:gravity="center"
                    android:fontFamily="@font/viga" android:background="@color/colorTableHeader"
                    android:textColor="#FFFFFF"
                    android:paddingLeft="4dp" android:paddingTop="8dp" android:paddingRight="4dp"
                    android:paddingBottom="8dp"
                    android:layout_weight=".8"/>
        </TableRow>
        <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:layout_weight="0">
            <androidx.recyclerview.widget.RecyclerView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:id="@+id/productListRecycle" android:layout_weight="1"/>
        </TableRow>
    </TableLayout>
    <TableLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tableLayout4" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.0" app:layout_constraintHorizontal_bias="0.0"
            android:id="@+id/tableLayout">

        <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            <TextView
                    android:text="@string/sf_quantity"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView9"
                    android:background="@color/colorTableHeader" android:textColor="#FFFFFF" android:textSize="14sp"
                    android:fontFamily="@font/viga" android:gravity="center" android:paddingLeft="4dp"
                    android:paddingTop="8dp" android:paddingRight="4dp" android:paddingBottom="8dp"
                    android:layout_weight=".2"/>
            <TextView
                    android:text="@string/sf_product_name"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView10"
                    android:background="@color/colorTableHeader" android:textColor="#FFFFFF" android:textSize="14sp"
                    android:fontFamily="@font/viga" android:gravity="center" android:paddingLeft="4dp"
                    android:paddingTop="8dp" android:paddingRight="4dp" android:paddingBottom="8dp"
                    android:layout_weight=".8"/>
        </TableRow>
        <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:layout_weight="0">
            <androidx.recyclerview.widget.RecyclerView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/produtListSaleRecycle" android:layout_weight="1"/>
        </TableRow>
    </TableLayout>
    <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:src="@drawable/ic_arrow_point_to_right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:id="@+id/btnCustomerSeleccion"
            android:focusable="true"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="24dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.95"
            app:layout_constraintVertical_bias="0.95"/>
</androidx.constraintlayout.widget.ConstraintLayout>